{
  "manifest_version": 3,
  "name": "Canvas AI Assistant",
  "version": "0.7.0",
  "description": "Automatically summarizes, classifies, and prioritizes Canvas assignments with Gemini Nano AI.",
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://uc.instructure.com/*",
    "https://uc.instructure.com/courses/*",
    "https://www.uc.edu/content/*"
    
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Canvas AI Assistant"
  },
  "content_scripts": [
    {
      "matches": ["https://uc.instructure.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
